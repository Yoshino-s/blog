{
  "compilerOptions": {
    "allowJs": false,
    "sourceMap": true,
    "target": "es6",
    "strict": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": [
      "quasar", "node"
    ],
    "paths": {
      "@assets/*": ["src/assets/*"]
    }
  },
  "exclude": ["node_modules"]
}
